{
  "name": "tesla-hero-slider",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "2.2.5",
    "node-sass-chokidar": "0.0.3",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/scss -o src/styles/scss",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/scss -o src/styles/scss --watch --recursive",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "eslint": "eslint .",
    "prettier": "prettier --write --tab-width 2 \"src/**/*.js\"",
    "precommit": "npm run prettier",
    "sass-lint": "sass-lint \"src/styles/scss/**/*.scss\" --verbose --no-exit"
  },
  "devDependencies": {
    "ajv": "6.2.1",
    "autoprefixer": "^8.1.0",
    "babel-eslint": "^8.2.2",
    "bezier-easing": "^2.0.3",
    "eslint": "4.18.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "7.7.0",
    "husky": "^0.15.0-rc.8",
    "lodash": "^4.17.5",
    "postcss-loader": "^2.1.1",
    "prettier": "^1.11.1",
    "raf": "^3.4.0",
    "react-animated-number": "0.4.4",
    "react-transition-group": "2.2.1",
    "sass-lint": "^1.12.1"
  }
}
